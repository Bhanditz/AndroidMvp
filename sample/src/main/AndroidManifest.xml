<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.idapgroup.android.mvpsample">

    <application>
        <activity
            android:name=".MainActivity"
            android:label="Mvp Sample"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SampleActivity"
            android:label="Presenter activity"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".SampleLceActivity"
            android:label="Presenter LCE activity"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".v2.SampleActivityV2"
            android:label="Presenter activity v2"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".v2.SampleLceActivityV2"
            android:label="Presenter new LCE activity v2"
            android:theme="@style/AppTheme" />

    </application>

</manifest>